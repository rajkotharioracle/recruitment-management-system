<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CandidatesEventHistory"
  Version="12.2.1.22.48"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.ViewObject.CandidatesEventHistory_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="eid"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="ceid"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT InterviewDetailsEO.CANDIDATE_EMAIL_ID, 
       InterviewDetailsEO.INTERVIEWER_EMAIL_ID, 
       InterviewDetailsEO.EVENT_ID, 
       InterviewDetailsEO.START_TIME, 
       InterviewDetailsEO.END_TIME, 
       InterviewDetailsEO.FEEDBACK_ID, 
       InterviewDetailsEO.ROUND_NO, 
       InterviewDetailsEO.STATUS, 
       InterviewerDataEO.INTERVIEWER_NAME, 
       InterviewerDataEO.INTERVIEWER_EMAIL
FROM  AKMR_INTERVIEW_DETAILS InterviewDetailsEO join AKMR_INTERVIEWER_DATA InterviewerDataEO
on InterviewerDataEO.INTERVIEWER_EMAIL=InterviewDetailsEO.INTERVIEWER_EMAIL_ID
where
InterviewDetailsEO.EVENT_ID=:eid
and
InterviewDetailsEO.CANDIDATE_EMAIL_ID=:ceid]]></SQLQuery>
  <EntityUsage
    Name="InterviewDetailsEO"
    Entity="model.EntityObject.InterviewDetailsEO"/>
  <EntityUsage
    Name="InterviewerDataEO"
    Entity="model.EntityObject.InterviewerDataEO"
    ReadOnly="true"
    Reference="false"
    DeleteParticipant="false">
    <DesignTime>
      <Attr Name="_customjoin" Value="true"/>
    </DesignTime>
  </EntityUsage>
  <ViewAttribute
    Name="CandidateEmailId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CandidateEmailId"
    EntityUsage="InterviewDetailsEO"
    AliasName="CANDIDATE_EMAIL_ID"/>
  <ViewAttribute
    Name="InterviewerEmailId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="InterviewerEmailId"
    EntityUsage="InterviewDetailsEO"
    AliasName="INTERVIEWER_EMAIL_ID"/>
  <ViewAttribute
    Name="EventId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="EventId"
    EntityUsage="InterviewDetailsEO"
    AliasName="EVENT_ID"/>
  <ViewAttribute
    Name="StartTime"
    PrecisionRule="true"
    EntityAttrName="StartTime"
    EntityUsage="InterviewDetailsEO"
    AliasName="START_TIME"/>
  <ViewAttribute
    Name="EndTime"
    PrecisionRule="true"
    EntityAttrName="EndTime"
    EntityUsage="InterviewDetailsEO"
    AliasName="END_TIME"/>
  <ViewAttribute
    Name="FeedbackId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="FeedbackId"
    EntityUsage="InterviewDetailsEO"
    AliasName="FEEDBACK_ID"/>
  <ViewAttribute
    Name="RoundNo"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="RoundNo"
    EntityUsage="InterviewDetailsEO"
    AliasName="ROUND_NO"/>
  <ViewAttribute
    Name="Status"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Status"
    EntityUsage="InterviewDetailsEO"
    AliasName="STATUS"/>
  <ViewAttribute
