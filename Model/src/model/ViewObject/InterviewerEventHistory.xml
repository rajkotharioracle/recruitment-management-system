    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="iid"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="fid"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT CandidateDataEO.CANDIDATE_EMAIL, 
       CandidateDataEO.CANDIDATE_NAME, 
       InterviewDetailsEO.CANDIDATE_EMAIL_ID, 
       InterviewDetailsEO.INTERVIEWER_EMAIL_ID, 
       InterviewDetailsEO.EVENT_ID, 
       InterviewDetailsEO.START_TIME, 
       InterviewDetailsEO.END_TIME, 
       InterviewDetailsEO.FEEDBACK_ID, 
       InterviewDetailsEO.ROUND_NO, 
       InterviewDetailsEO.STATUS, 
       FeedbackDetailsEO.FEEDBACK_ACADEMIC_RIGOR, 
       FeedbackDetailsEO.FEEDBACK_ID AS FEEDBACK_ID1, 
       FeedbackDetailsEO.FEEDBACK_TIMESTAMP, 
       FeedbackDetailsEO.FEEDBACK_TECHNICAL_SKILLS, 
       FeedbackDetailsEO.FEEDBACK_PROBLEM_SOLVING, 
       FeedbackDetailsEO.FEEDBACK_PERSONAL_SKILLS, 
       FeedbackDetailsEO.FEEDBACK_OBSERVATIONS, 
       FeedbackDetailsEO.FEEDBACK_OVERALL_ASSESSMENT, 
       FeedbackDetailsEO.FEEDBACK_RESULT
FROM   AKMR_CANDIDATE_DATA CandidateDataEO
join AKMR_INTERVIEW_DETAILS InterviewDetailsEO
on CandidateDataEO.CANDIDATE_EMAIL = InterviewDetailsEO.CANDIDATE_EMAIL_ID
join AKMR_FEEDBACK_DETAILS FeedbackDetailsEO 
on FeedbackDetailsEO.FEEDBACK_ID=InterviewDetailsEO.FEEDBACK_ID
where
InterviewDetailsEO.INTERVIEWER_EMAIL_ID=:iid
and
InterviewDetailsEO.EVENT_ID=:eid
and
FeedbackDetailsEO.FEEDBACK_RESULT=:fid]]></SQLQuery>
