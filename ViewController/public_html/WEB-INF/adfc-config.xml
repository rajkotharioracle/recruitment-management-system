<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="Login"></view>
  <view id="adminHome">
    <page>/adminHome.jsf</page>
  </view>
  <view id="eventDetail">
    <page>/eventDetail.jsf</page>
  </view>
  <view id="Candidate_Lookup"></view>
  <view id="Interviewer_Lookup"></view>
  <view id="Report">
    <page>/Report.jsf</page>
  </view>
  <view id="createInterview">
    <page>/createInterview.jsf</page>
  </view>
  <method-call id="getEventidforAssigning">
    <method>#{bindings.getEventidforAssigning.execute}</method>
    <outcome>
      <fixed-outcome>getEventidforAssigning</fixed-outcome>
    </outcome>
  </method-call>
  <view id="event_info">
    <page>/event_info.jsf</page>
  </view>
  <view id="event_create">
    <page>/event_create.jsf</page>
  </view>
  <view id="event_create1">
    <page>/event_create1.jsf</page>
  </view>
  <view id="event_create2">
    <page>/event_create2.jsf</page>
  </view>
  <view id="event_create3">
    <page>/event_create3.jsf</page>
  </view>
  <view id="interviewer_dashboard">
    <page>/interviewer_dashboard.jsf</page>
  </view>
  <view id="viewcandidate">
    <page>/viewcandidate.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>Login</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>login1</from-outcome>
      <to-activity-id>adminHome</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__3">
      <from-outcome>login2</from-outcome>
      <to-activity-id>interviewer_dashboard</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__4">
    <from-activity-id>adminHome</from-activity-id>
    <control-flow-case id="__6">
      <from-outcome>gotoCandidate</from-outcome>
      <to-activity-id>Candidate_Lookup</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__7">
      <from-outcome>generateReport</from-outcome>
      <to-activity-id>Report</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__9">
      <from-outcome>gotoInterviewer</from-outcome>
      <to-activity-id>Interviewer_Lookup</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__8">
      <from-outcome>select</from-outcome>
      <to-activity-id>eventDetail</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__5">
      <from-outcome>event</from-outcome>
      <to-activity-id>event_info</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__17">
    <from-activity-id>eventDetail</from-activity-id>
    <control-flow-case id="__18">
      <from-outcome>assigning</from-outcome>
      <to-activity-id>getEventidforAssigning</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__19">
    <from-activity-id>getEventidforAssigning</from-activity-id>
    <control-flow-case id="__20">
      <from-outcome>getEventidforAssigning</from-outcome>
      <to-activity-id>createInterview</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__10">
    <from-activity-id>event_info</from-activity-id>
    <control-flow-case id="__11">
      <from-outcome>create1</from-outcome>
      <to-activity-id>event_create</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__28">
    <from-activity-id>event_create1</from-activity-id>
    <control-flow-case id="__29">
      <from-outcome>create3</from-outcome>
      <to-activity-id>event_create2</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__30">
    <from-activity-id>event_create2</from-activity-id>
    <control-flow-case id="__31">
      <from-outcome>create4</from-outcome>
      <to-activity-id>event_create3</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__32">
    <from-activity-id>event_create</from-activity-id>
    <control-flow-case id="__33">
      <from-outcome>create2</from-outcome>
      <to-activity-id>event_create1</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__12">
    <from-activity-id>interviewer_dashboard</from-activity-id>
    <control-flow-case id="__13">
      <from-outcome>candidatedet</from-outcome>
      <to-activity-id>viewcandidate</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__16">
    <managed-bean-name>Eventselection</managed-bean-name>
    <managed-bean-class>view.bean.EventSelection</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__21">
    <managed-bean-name>CreateInterview</managed-bean-name>
    <managed-bean-class>view.bean.CreateInterview</managed-bean-class>
    </managed-bean>
  <managed-bean id="__22">
    <managed-bean-name>setCompletedEvent</managed-bean-name>
    <managed-bean-class>view.Event</managed-bean-class>
    
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__23">
    <managed-bean-name>interviewassign</managed-bean-name>
    <managed-bean-class>view.bean.interviewassign</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</adfc-config>
